{
  "presets": [
    "@babel/preset-typescript",
    "next/babel"
  ],
  "plugins": [
    [
      "prismjs",
      {
        "languages": [
          "javascript",
          "css",
          "typescript",
          "json",
          "jsx",
          "markdown",
          "bash",
          "yaml"
        ],
        "plugins": ["line-numbers"],
        "theme": "tomorrow",
        "css": true
      }
    ]
  ]
}